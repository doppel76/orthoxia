-- Scripts:

local function KHKGY_fake_script() -- TextButton.Script 
	local script = Instance.new('Script', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		print("Executed lag script x_x")
		print("made by doppel#4095")
		print("Executed lag script x_x")
print("made by doppel#4095")
            while true do
                local args = {
                    [1] = "Equip"
                }
                game:GetService("Players").LocalPlayer.Character:FindFirstChild("7").Remote:FireServer(unpack(args))
                local args = {
                    [1] = "UnEquip"
                }
                game:GetService("Players").LocalPlayer.Character:FindFirstChild("7").Remote:FireServer(unpack(args))
                game:GetService("RunService").RenderStepped:Wait()
            end
	end)
end
coroutine.wrap(KHKGY_fake_script)()
local function FPQFQI_fake_script() -- ImageButton.Script 
	local script = Instance.new('Script', ImageButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent:Destroy()
		end)
end
coroutine.wrap(FPQFQI_fake_script)()
